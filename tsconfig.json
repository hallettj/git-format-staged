{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    //"outDir": "./build",
    //"rootDir": "./test",
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
